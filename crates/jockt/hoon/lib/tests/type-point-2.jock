// Test class with multiple methods accessing self fields
struct PointState { x: Real, y: Real };
class Point(PointState) {
  getx(self: PointState) -> Real {
    self.x
  }
  gety(self: PointState) -> Real {
    self.y
  }
};

let point1 = Point { x: 104, y: 62 };
let point2 = Point { x: 50, y: 8 };
( (point1.getx() point1.gety())
  (point2.getx() point2.gety())
)
