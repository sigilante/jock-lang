// Test class with multiple methods and different return types
struct PointState { x: Real, y: Real };
class Point(PointState) {
  getx(self: PointState) -> Real {
    self.x
  }
  gety(self: PointState) -> Real {
    self.y
  }
  inc(self: PointState, q: Real) -> Real {
    +(q)
  }
};

let one = Point { x: 32, y: 10 };
let two = one.inc(41);
((one.getx() one.gety()) two)

