/*  A class is broadly equivalent to a Hoon door. It has a top-level
    sample which represents its state, along with methods that have
    each their own samples.
*/
struct FooState { x: Real };
class Foo(FooState) {
  bar(self: FooState, p: Real) -> Real {
    p
  }
};

//  let name:Type = value;
let a = Foo { x: 41 };
//  let name = Type { fields };
let b = Foo { x: 42 };
//  let name:type = value;
let c:@ = 43;

(Foo { x: 40 } a b c)
